{
  FILE *f=fopen("conftestval", "w");
  if (!f) exit(1);
  fprintf(f, "%d\n", sizeof(ino_t));
  exit(0);
}
_ACEOF
if ac_fn_c_try_run "$LINENO"; then :
  ac_cv_sizeof_ino_t=`cat conftestval`
else
  ac_cv_sizeof_ino_t=0
fi
