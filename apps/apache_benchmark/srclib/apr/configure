  $EGREP "YES_IS_DEFINED" >/dev/null 2>&1; then :
  apr_cv_define_INT64_C=yes
else

    # Now check for INT64_C in stdint.h
    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <stdint.h>
#ifdef INT64_C
YES_IS_DEFINED
#endif
_ACEOF
